{
  "manifest_version": 3,
  "name": "Vite.js Chrome Extension",
  "version": "1.0",
  "description": "A Chrome Extension built with Vite.js",
  "action": {
    "default_title": "Vite.js Chrome Extension Title",
    "default_popup": "index.html",
    "default_icon": "icon.png"
  },
  "permissions": ["scripting"],
  "host_permissions": ["http://*/*", "https://*/*"]
}
